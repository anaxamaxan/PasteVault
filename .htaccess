<IfModule mod_rewrite.c>
RewriteEngine on
RewriteBase /

RewriteEngine On
RewriteCond %{HTTP_HOST} !^www\.pastevault\.com$ [NC]
RewriteRule ^(.*)$ https://www.pastevault.com/$1 [R=301,L]     

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.*)$ index.php/$1 [L]
</IfModule>